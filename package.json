{
	"name": "bar-interdimensional",
	"version": "1.0.0",
	"description": "El mejor Bar Interdimensional del universo",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/Germallet/BarInterdimensional"
	},
	"author": "",
	"main": "build/inicio.js",
	"engines": {
		"node": "13.x"
	},
	"scripts": {
		"build": "tsc & eslint src/**/*.{js,ts}",
		"build-and-run": "npm run build & npm run start-dev",
		"npm-install": "npm install",
		"postinstall": "npm run build",
		"prisma-generate": "npx prisma generate",
		"prisma-introspect": "npx prisma introspect",
		"start": "SET NODE_ENV=production & node build/inicio.js",
		"start-dev": "heroku local"
	},
	"dependencies": {
		"@prisma/client": "^2.5.0",
		"@types/node": "^14.6.0",
		"@types/ws": "^7.2.6",
		"async-mutex": "^0.2.4",
		"discord.js": "^12.3.1",
		"sxml": "^1.0.4",
		"web-request": "^1.0.7"
	},
	"devDependencies": {
		"@prisma/cli": "^2.5.0",
		"@typescript-eslint/eslint-plugin": "^3.9.1",
		"@typescript-eslint/parser": "^3.9.1",
		"eslint": "^7.7.0",
		"eslint-config-prettier": "^6.11.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.0.5",
		"typescript": "^3.9.7"
	},
	"prettier": {
		"printWidth": 1000,
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"quoteProps": "consistent",
		"trailingComma": "none",
		"bracketSpacing": true,
		"arrowParens": "always",
		"proseWrap": "preserve"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"extends": [
			"plugin:@typescript-eslint/recommended",
			"prettier/@typescript-eslint"
		],
		"parserOptions": {
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"rules": {}
	}
}
