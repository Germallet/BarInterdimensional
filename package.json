{
	"name": "bar-interdimensional",
	"version": "1.0.0",
	"description": "El mejor Bar Interdimensional del universo",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/Germallet/BarInterdimensional"
	},
	"author": "",
	"main": "build/inicio.js",
	"engines": {
		"node": "13.x"
	},
	"scripts": {
		"build": "tsc & eslint src/**/*.{js,ts}",
		"build-and-run": "npm run build & npm run start-dev",
		"npm-install": "npm install",
		"heroku-postinstall": "npm run build",
		"prisma-generate": "npx prisma generate",
		"prisma-introspect": "npx prisma introspect",
		"start": "node -r ./paths.js build/inicio.js",
		"start-dev": "node -r dotenv/config -nolazy --inspect -r ./paths.js build/inicio.js",
		"test": "ts-mocha --inspect --paths tests/**/*.ts"
	},
	"dependencies": {
		"@prisma/client": "^2.6.0",
		"@types/node": "^14.6.3",
		"@types/ws": "^7.2.6",
		"async-mutex": "^0.2.4",
		"discord.js": "^12.3.1",
		"sxml": "^1.0.4",
		"tsconfig-paths": "^3.9.0",
		"web-request": "^1.0.7"
	},
	"devDependencies": {
		"@prisma/cli": "^2.6.0",
		"@types/chai": "^4.2.12",
		"@types/chai-as-promised": "^7.1.3",
		"@types/mocha": "^8.0.3",
		"@typescript-eslint/eslint-plugin": "^4.0.1",
		"@typescript-eslint/parser": "^4.0.1",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"dotenv": "^8.2.0",
		"eslint": "^7.8.1",
		"eslint-config-prettier": "^6.11.0",
		"mocha": "^8.1.3",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.1",
		"ts-mocha": "^7.0.0",
		"ts-mockito": "^2.6.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.2"
	},
	"prettier": {
		"printWidth": 1000,
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"singleQuote": true,
		"quoteProps": "consistent",
		"trailingComma": "none",
		"bracketSpacing": true,
		"arrowParens": "always",
		"proseWrap": "preserve"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"extends": [
			"plugin:@typescript-eslint/recommended",
			"prettier/@typescript-eslint"
		],
		"parserOptions": {
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"rules": {}
	}
}
