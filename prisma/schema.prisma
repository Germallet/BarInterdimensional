generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model configuracion {
  identificador String  @id
  valor         String?
  numerico      Boolean
}

model mundo {
  id    Float   @default(dbgenerated()) @id
  guild String?
  nodo  nodo[]
}

model nodo {
  id                Float   @default(dbgenerated()) @id
  mundo             Float
  nombre            String
  canalvoz          String?
  cantaltexto       String?
  mundo_mundoTonodo mundo   @relation(fields: [mundo], references: [id])
}
